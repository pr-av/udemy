package com.example.demo.Service;

import java.util.List;

import com.example.demo.model.Units;

public interface UnitsService {
	  
	List<Units> getAllUnits();
	    Units getUnitByName(String name);
	    List<Units> getActiveUnits();
	    Units createUnit(Units unit);
	    Units updateUnitActiveStatus(String name, boolean active);
	    void deleteUnit(String name);
}
